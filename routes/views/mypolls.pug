html
  head
    title=user
    style
      include main.css
    script
      include mypolls.js
    script(src="https://code.jquery.com/jquery-1.11.3.min.js")
  body
  #upperheader
    ul(class="upperbuttons")
      li: a(href="/dashboard") #{user}
      li: a#logout(href='/logout') Logout
  h4(id='notification')=message
  #useroptions
    ul(class="upperbuttonsleft")
      li: a(href='/createPoll') Create a New Poll
      li: a(href='/dashboard') Dashboard
    br
    p Your Polls
    hr
  div#spinner(class="spinner")
  #pollList
